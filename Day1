#Lists
#Lists are mutable

x=[4,True,'hi']
print(len(x))
y='hi'
print(len(y))
x.append('hello')
x.extend([4,5,5])
print(x.pop())
print(x.pop(0)) #based on index
print(x[1])
x[0]='hello' #Lists are mutable
print(x)
y=x[:]
print(x,y)

