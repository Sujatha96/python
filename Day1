x = lambda x: x+5
print(x(2))

x = lambda x,y: x+y
print(x(2,5))

#Maps and Filters

x=[1,2,3,4,5,6,7,8,9]
mp=map(lambda i: i*2, x)
print(list(mp))

x=[1,2,3,4,5,6,7,8,9]
ft=filter(lambda i: i%2==0, x)
print(list(ft))

a=18
x=f'hello {6+8} {20+8}'
print(f'hello {a}')
print(x)
