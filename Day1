#For loop


for i in range(10):
    print(i)

#for range function
#start stop step
#Ex: range(1,10,2): iterates alternatively as step is 2

for i in [1,2,3,4,5]:
    print(i)

x=[2,3,4,5,6]
for i in x:
    print(i)
for i in range(len(x)):
    print(x[i])
for i,element in enumerate(x):
    print(i,element)


