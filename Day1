func()

def func(x,y,z=None):
    print('Run',x,y,z)
    return x*y,x/y

print(func(3,5,7))
r1,r2=func(3,5)
print(r1,r2)

def func(x):
    def func2():
        print(x)
    return func2

print(func(3)())
print(func(3))
x=func(3)
x()

def func(*args,**kwargs):
    pass

x=[1,23,3,4,5]
print(x)
print(*x)

def func(x,y):
    print(x,y)

pairs=[(1,2),(3,4)]

for pair in pairs:
    func(*pair)

for pair in pairs:
    func(**{'x':2,'y':5})


def func(*args,**kwargs):
    print(args,kwargs)
    print(*args)
    print(**kwargs)

func(1,2,3,4,5,one=0,two=1)

x='abc'

def func(name):
    global x
    x=name


print(x)
func('changed')
print(x)

